---
export const prerender = false;

import BaseLayout from "../../../layouts/BaseLayout.astro";
import OnboardToken from "../../../components/console/onboard/OnboardToken";
import OnboardSteps, {
	OnboardStep,
} from "../../../components/console/onboard/OnboardSteps";

// Using `meta.env` requires `prerender = false`
const BENCHER_API_URL = import.meta.env.BENCHER_API_URL;
---

<BaseLayout
  title="API Token"
  description="Setup a new API token for Bencher"
>
<div slot="navbar" />

<OnboardToken client:only="solid-js" apiUrl={BENCHER_API_URL} >
  <OnboardSteps slot="fallback" step={OnboardStep.API_TOKEN} />
</OnboardToken>

<div slot="footer" />
</BaseLayout>