---
export const prerender = false;

import BaseLayout from "../../../layouts/BaseLayout.astro";
import OnboardProject from "../../../components/console/onboard/OnboardProject";
import OnboardSteps, { OnboardStep } from "../../../components/console/onboard/OnboardSteps";

// Using `meta.env` requires `prerender = false`
const BENCHER_API_URL = import.meta.env.BENCHER_API_URL;
---

<BaseLayout
  title="Project Name"
  description="Setup a new Project on Bencher"
>
<div slot="navbar" />

<OnboardProject client:only="solid-js" apiUrl={BENCHER_API_URL}>
  <OnboardSteps slot="fallback" step={OnboardStep.PROJECT} />
</OnboardProject>

<div slot="footer" />
</BaseLayout>